doctype html
html
  head
    meta(charset='UTF-8')
    title Task manager UI
    link(rel='stylesheet', href='/css/style.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/stylesheets/bootstrap.css')
    link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
    script(src='/javascripts/angular.js')
    script(src='/javascripts/angular.min.js.map')
    script(src='/javascripts/angular-cookies.js')
    script(src='/javascripts/angular-cookies.min.js')
    script(src='/javascripts/script.js')
  body
    .page
      .pageHeader
        .title Dashboard
        .userPanel
          span.username
            a(href='/logout') Logout 
      .main
        .nav
          .menu
            .title Navigation
            ul
              li
                i.fa.fa-home
                a(href='/home')  Home
              li.active
                i.fa.fa-tasks
                a(href='/setting')  Token Setting
              li
                i.fa.fa-tasks
                a(href='/pingSetting')  Ping Setting
        .view
          .viewHeader
            .title Set the following info
            div(ng-app='myApp',ng-controller='tokenController')
              br
              .form-group.row
                .col-xs-7
                  label Token Id
                  input.form-control(type='text' placeholder='Token Id' ng-model='data.tokenId')
                br
                .col-xs-7
                  label Chat Id
                  input.form-control(type='text' placeholder='Chagt Id' ng-model='data.chatId')
                  br
                .col-xs-7
                  button.btn.btn-primary.btn-block(type='button',ng-click='submit()') SUBMIT
    script(src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js')
    script(src='js/index.js')
  
